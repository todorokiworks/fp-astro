---
import Layout from "../../../../../layouts/Layout.astro";
import structuredDataMarkup from "../../../../../data/data.json";
const markup = JSON.stringify(structuredDataMarkup.promotionalDesignTag);
---

<Layout
    title="飲食店 商品タグ制作事例｜テンポスフードプレイス"
    description="飲食店の商品タグの制作事例をご紹介。テンポスではオープンチラシや折り込みチラシなど販促に必要なチラシやポスター、ショップカードなど作成しております。"
    pageUrl="/promotion/promotional_design/jirei/tag/"
    ogimage="/asset/images/promotional_design/ogp_promotional_design.png"
    markup={markup}
>
    <article id="mainarea">
        <div class="tp-border-new sans">
            <nav class="tp-nav">
                <ol class="tp-nav__list flex">
                    <li><a href="/">トップページ</a></li>
                    <li><a href="/promotion/">販促デザイン</a></li>
                    <li>
                        <a href="/promotion/promotional_design/">販促物作成</a>
                    </li>
                    <li>
                        <a href="/promotion/promotional_design/jirei"
                            >販促物制作事例</a
                        >
                    </li>
                    <li>商品タグ</li>
                </ol>
            </nav>
            <section class="btm-front__section promotionlist">
                <h1 class="btm-front__ttl font_m_plus">
                    <span>商品タグ</span>
                </h1>
            </section>
            <section class="iz-case__section g-wrapper btm-sec">
                <ul class="iz-case__list">
                    <li>
                        <p class="iz-case__list__thumb">
                            <a
                                href="/asset/images/promotiondesign-jirei/tag01.jpg"
                                data-lightbox="image-1"
                                data-title=""
                            >
                                <picture>
                                    <source
                                        media="(max-width:767px)"
                                        srcset="/asset/images/promotiondesign-jirei/tag01.jpg"
                                    />
                                    <img
                                        src="/asset/images/promotiondesign-jirei/tag01.jpg"
                                        srcset="/asset/images/promotiondesign-jirei/tag01.jpg 1x,/asset/images/promotiondesign-jirei/tag01.jpg 2x"
                                        alt="SOYBREAKまめcafe"
                                        width="206"
                                        height="133"
                                        class="imgauto"
                                    />
                                </picture>
                            </a>
                        </p>
                    </li>
                </ul>
                <a
                    href="/promotion/promotional_design/jirei/"
                    class="jirei-backbtn">販促物制作事例一覧に戻る</a
                >
            </section>
        </div>
    </article>
</Layout>

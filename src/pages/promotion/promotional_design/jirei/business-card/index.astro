---
import Layout from "../../../../../layouts/Layout.astro";
import structuredDataMarkup from "../../../../../data/data.json";
const markup = JSON.stringify(
    structuredDataMarkup.promotionalDesignBusinessCard,
);
---

<Layout
    title="飲食店 名刺制作事例 | テンポスフードプレイス"
    description="飲食店の名刺制作事例をご紹介。テンポスではオープンチラシや折り込みチラシなど販促に必要なチラシやポスター、ショップカードなど作成しております。"
    pageUrl="/promotion/promotional_design/jirei/buisiness-card/"
    ogimage=""
    markup={markup}
>
    <article id="mainarea">
        <div class="tp-border-new sans">
            <nav class="tp-nav">
                <ol class="tp-nav__list flex">
                    <li><a href="/">トップページ</a></li>
                    <li><a href="/promotion/">販促デザイン</a></li>
                    <li>
                        <a href="/promotion/promotional_design/">販促物作成</a>
                    </li>
                    <li>
                        <a href="/promotion/promotional_design/jirei"
                            >販促物制作事例</a
                        >
                    </li>
                    <li>名刺</li>
                </ol>
            </nav>
            <section class="btm-front__section promotionlist">
                <h1 class="btm-front__ttl font_m_plus">
                    <span>名刺</span>
                </h1>
            </section>
            <section class="iz-case__section g-wrapper btm-sec">
                <ul class="iz-case__list">
                    <li>
                        <p class="iz-case__list__thumb">
                            <a
                                href="/asset/images/promotiondesign-jirei/bc01.jpg"
                                data-lightbox="image-1"
                                data-title=""
                            >
                                <picture>
                                    <source
                                        media="(max-width:767px)"
                                        srcset="/asset/images/promotiondesign-jirei/bc01.jpg"
                                    />
                                    <img
                                        src="/asset/images/promotiondesign-jirei/bc01.jpg"
                                        srcset="/asset/images/promotiondesign-jirei/bc01.jpg 1x,/asset/images/promotiondesign-jirei/bc01.jpg 2x"
                                        alt="peace"
                                        width="206"
                                        height="133"
                                        class="imgauto"
                                    />
                                </picture>
                            </a>
                        </p>
                    </li>
                    <li>
                        <p class="iz-case__list__thumb">
                            <a
                                href="/asset/images/promotiondesign-jirei/bc02.jpg"
                                data-lightbox="image-2"
                                data-title=""
                            >
                                <picture>
                                    <source
                                        media="(max-width:767px)"
                                        srcset="/asset/images/promotiondesign-jirei/bc02.jpg"
                                    />
                                    <img
                                        src="/asset/images/promotiondesign-jirei/bc02.jpg"
                                        srcset="/asset/images/promotiondesign-jirei/bc02.jpg 1x,/asset/images/promotiondesign-jirei/bc02.jpg 2x"
                                        alt="お酒と旬菜一品 庵"
                                        width="206"
                                        height="133"
                                        class="imgauto"
                                    />
                                </picture>
                            </a>
                        </p>
                    </li>
                    <li>
                        <p class="iz-case__list__thumb">
                            <a
                                href="/asset/images/promotiondesign-jirei/bc03.jpg"
                                data-lightbox="image-3"
                                data-title=""
                            >
                                <picture>
                                    <source
                                        media="(max-width:767px)"
                                        srcset="/asset/images/promotiondesign-jirei/bc03.jpg"
                                    />
                                    <img
                                        src="/asset/images/promotiondesign-jirei/bc03.jpg"
                                        srcset="/asset/images/promotiondesign-jirei/bc03.jpg 1x,/asset/images/promotiondesign-jirei/bc03.jpg 2x"
                                        alt="桔きょう"
                                        width="206"
                                        height="133"
                                        class="imgauto"
                                    />
                                </picture>
                            </a>
                        </p>
                    </li>
                    <li>
                        <p class="iz-case__list__thumb">
                            <a
                                href="/asset/images/promotiondesign-jirei/bc04.jpg"
                                data-lightbox="image-4"
                                data-title=""
                            >
                                <picture>
                                    <source
                                        media="(max-width:767px)"
                                        srcset="/asset/images/promotiondesign-jirei/bc04.jpg"
                                    />
                                    <img
                                        src="/asset/images/promotiondesign-jirei/bc04.jpg"
                                        srcset="/asset/images/promotiondesign-jirei/bc04.jpg 1x,/asset/images/promotiondesign-jirei/bc04.jpg 2x"
                                        alt="亀ひろ～フードSHOP～"
                                        width="206"
                                        height="133"
                                        class="imgauto"
                                    />
                                </picture>
                            </a>
                        </p>
                    </li>
                    <li>
                        <p class="iz-case__list__thumb">
                            <a
                                href="/asset/images/promotiondesign-jirei/bc05.jpg"
                                data-lightbox="image-5"
                                data-title=""
                            >
                                <picture>
                                    <source
                                        media="(max-width:767px)"
                                        srcset="/asset/images/promotiondesign-jirei/bc05.jpg"
                                    />
                                    <img
                                        src="/asset/images/promotiondesign-jirei/bc05.jpg"
                                        srcset="/asset/images/promotiondesign-jirei/bc05.jpg 1x,/asset/images/promotiondesign-jirei/bc05.jpg 2x"
                                        alt="chat noir"
                                        width="206"
                                        height="133"
                                        class="imgauto"
                                    />
                                </picture>
                            </a>
                        </p>
                    </li>
                </ul>
                <a
                    href="/promotion/promotional_design/jirei/"
                    class="jirei-backbtn">販促物制作事例一覧に戻る</a
                >
            </section>
        </div>
    </article>
</Layout>

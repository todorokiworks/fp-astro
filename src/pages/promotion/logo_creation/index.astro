                            <tr>
                                <th>会社名</th>
                                <td>テンポスフードプレイス</td>
                                <td>A社</td>
                                <td>B社</td>
                            </tr>
                            <tr>
                                <th>デザイン費用</th>
                                <td>16,500円〜</td>
                                <td>89,000円</td>
                                <td>300,000円</td>
                            </tr>
                            <tr>

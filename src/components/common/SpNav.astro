<script is:inline>
    document.addEventListener("DOMContentLoaded", function () {
        class FatNav {
            constructor(options = {}) {
                this.settings = Object.assign({}, FatNav.defaults, options);
                this._name = "fatNav";
                this.init();
            }

            init() {
                this.$nav = document.querySelector(".fat-nav");
                this.$hamburger = document.createElement("a");
                this.$hamburger.href = "javascript:void(0)";
                this.$hamburger.classList.add("hamburger");
                this.$hamburger.innerHTML =
                    '<div class="hamburger__icon"></div>';

                this._bodyOverflow = document.body.style.overflow;

                /* if (/iPad|iPhone|iPod/.test(navigator.userAgent)) {
                    this.$nav.children[0].style.height = "110%";
                    this.$nav.children[0].style.transform = "translateY(-5%)";
                }*/

                document.body.appendChild(this.$hamburger);

                // クリックイベントを登録
                [this.$hamburger, ...this.$nav.querySelectorAll("a")].forEach(
                    (el) => {
                        el.addEventListener("click", () => this.toggleNav());
                    },
                );
            }

            toggleNav() {
                this.$nav.classList.toggle("active");
                this.$nav.style.display = this.$nav.classList.contains("active")
                    ? "block"
                    : "none";
                this.toggleBodyOverflow();
                this.$hamburger.classList.toggle("active");
            }

            toggleBodyOverflow() {
                document.body.classList.toggle("no-scroll");
                const isNoScroll =
                    document.body.classList.contains("no-scroll");
                document.body.style.overflow = isNoScroll
                    ? "hidden"
                    : this._bodyOverflow;
            }
        }

        FatNav.defaults = {};

        if (typeof window.fatNav === "undefined") {
            window.fatNav = function (options) {
                return new FatNav(options);
            };
        }

        document.addEventListener("click", function (event) {
            const target = event.target;
            if (
                document.body.classList.contains("no-scroll") &&
                target.classList.contains("no-scroll")
            ) {
                document.body.classList.remove("no-scroll");
                document.body.style.overflow = "visible";
                document.querySelector(".fat-nav").classList.remove("active");
                document.querySelector(".hamburger").classList.remove("active");

                // `.hamburger-ps` に `.hamburger` を追加（存在する場合）
                const hamburgerPs = document.querySelector(".hamburger-ps");
                if (hamburgerPs) {
                    hamburgerPs.appendChild(
                        document.querySelector(".hamburger"),
                    );
                }
            }
        });

        // FatNav インスタンスを作成
        fatNav();
    });
</script>
